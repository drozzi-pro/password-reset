msgstr ""

#: inc/api/api.route.reset-password.php:22
#: inc/api/api.route.set-password.php:22
#: inc/api/api.route.validate-code.php:22
msgid "You must provide an email address."
msgstr "Вы должны ввести email"

#: inc/api/api.route.reset-password.php:28
#: inc/api/api.route.set-password.php:36
#: inc/api/api.route.validate-code.php:32
msgid "No user found with this email address."
msgstr "Пользователь с таким email не найден"

#: inc/api/api.route.reset-password.php:42
msgid "A password reset email has been sent to your email address."
msgstr "Письмо с кодом пришло на ваш email"

#: inc/api/api.route.set-password.php:26
#: inc/api/api.route.validate-code.php:26
msgid "You must provide a code."
msgstr "Вы должны ввести код"

#: inc/api/api.route.set-password.php:30
msgid "You must provide a new password."
msgstr "Вы должны ввести новый пароль"

#: inc/api/api.route.set-password.php:50
msgid "Password reset successfully."
msgstr "Пароль успешно изменён"

#: inc/api/api.route.validate-code.php:48
msgid "The code supplied is valid."
msgstr "Предоставленный код верен"

#: inc/class/class.user.php:26
msgid "You must provide a $user_id to initiate a BDPWR_User object."
msgstr ""

#: inc/class/class.user.php:58
msgid "You cannot request a password reset for a user with this role."
msgstr "Вы не можете поменять пароль у пользователя с этой ролью"

#: inc/class/class.user.php:93
msgid "There was a problem validating the code."
msgstr "Возникли проблемы с проверкой кода"

#: inc/class/class.user.php:117
msgid "You must request a password reset code before you try to set a new password."
msgstr "Вы должны запросить новый код"

#. translators: %s: Number of remaining attempts
#: inc/class/class.user.php:144
msgid "You have %s attempts remaining."
msgstr "У вас осталось %s попыток"

#: inc/class/class.user.php:150
msgid "You have used the maximum number of attempts allowed. You must request a new code."
msgstr "Вы достигли максимального количества попыток. Запросите новый код"

#: inc/class/class.user.php:152
#: inc/class/class.user.php:156
msgid "The reset code provided is not valid. "
msgstr "Введённый код неверен"

#: inc/class/class.user.php:161
msgid "The reset code provided is not valid."
msgstr "Введённый код неверен"

#: inc/class/class.user.php:176
msgid "The reset code provided has expired."
msgstr "Действие введённого кода истекло. Запросите новый"

#: inc/functions.php:152
msgid "An email address is required for the reset code email."
msgstr "Для отправки письма с кодом требуется email"

#: inc/functions.php:157
msgid "No code was provided for the password reset email."
msgstr "Код для сбраса пароля не был предоставлен"
